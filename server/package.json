    "eslint-plugin-prettier": "^4.2.1",
    "find-free-ports": "^3.1.1",
    "grpc-tools": "^1.12.4",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "socket.io-client": "^4.7.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "ts-proto": "^1.156.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "uuid": "^9.0.0"
  }
}
