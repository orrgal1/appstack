{
  "name": "main-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "proto:combine": "cd src && node ../proto/protoCombine.js && cd .. && cp src/proto/combined.proto proto/combined.proto",
    "proto:compile": "protoc --plugin=$(pwd)/node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=snakeToCamel=true,outputClientImpl=false -I=./proto --ts_proto_out=./proto combined.proto",
    "proto:client": "cd proto && ../node_modules/.bin/grpc_tools_node_protoc --plugin=protoc-gen-ts_proto=../node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./client --ts_proto_opt=outputServices=nice-grpc,outputServices=generic-definitions,useExactTypes=false --proto_path=. combined.proto && cd .. && cp proto/client/combined.ts src/libs/client/index.ts",
    "proto:interfaces": "cd proto && node protoInterfaces.js && cd .. && eslint --fix proto/interfaces.ts && cp proto/interfaces.ts src/proto",
    "proto:build": "npm run proto:combine && npm run proto:compile && npm run proto:interfaces",
    "proto:build:client": "npm run proto:build && npm run proto:client",
    "start:ts": "ts-node src/main/index.ts",
    "setup:ts": "bash infra/setup.sh && ts-node infra/setup/runSetup.ts",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:e2e": "E2E=1 SERVICE_HOST=service jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.18",
    "@grpc/proto-loader": "^0.7.8",
    "@jenniferplusplus/opentelemetry-instrumentation-bullmq": "^0.3.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/microservices": "^10.0.5",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/terminus": "^10.1.1",
    "@opentelemetry/auto-instrumentations-node": "^0.39.2",
    "@opentelemetry/exporter-prometheus": "^0.43.0",
    "@opentelemetry/instrumentation-express": "^0.33.1",
    "@opentelemetry/instrumentation-grpc": "^0.43.0",
    "@opentelemetry/instrumentation-http": "^0.43.0",
    "@opentelemetry/instrumentation-nestjs-core": "^0.33.1",
    "@opentelemetry/instrumentation-redis": "^0.35.1",
    "@opentelemetry/resources": "^1.17.0",
    "@opentelemetry/sdk-node": "^0.43.0",
    "@opentelemetry/sdk-trace-base": "^1.17.0",
    "@opentelemetry/semantic-conventions": "^1.17.0",
    "@socket.io/redis-adapter": "^8.2.1",
    "@types/google-protobuf": "^3.15.6",
    "arangojs": "^8.1.0",
    "bottleneck": "^2.19.5",
    "bullmq": "^4.12.0",
    "dotenv": "^16.0.3",
    "express-session": "^1.17.3",
    "google-protobuf": "^3.21.2",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "ioredis": "^5.3.1",
    "jsonwebtoken": "^9.0.2",
    "lodash.clonedeep": "^4.5.0",
    "long": "^5.2.3",
